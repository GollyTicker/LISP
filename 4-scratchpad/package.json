{
  "name": "4-scratchpad",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate-static-assets": "echo Generating default.lisp... ; cp ../1-interpreter/standard-library.lisp src/assets/default.lisp; echo -n 'DONE: '; ls src/assets",
    "preserve": "npm run generate-static-assets",
    "serve": "vue-cli-service serve",
    "prebuild": "npm run generate-static-assets",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "splitpanes": "^3.0.4",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "raw-loader": "^4.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
